{
    "name": "sshextension",
    "displayName": "SSHExtension",
    "description": "Extension for SSH connections using the ftp-simple settings",
    "version": "0.0.2",
    "publisher": "kondratiev",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openConnection",
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.openConnection",
            "title": "Open SSH Connection"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "extensionDependencies": [
        "humy2833.ftp-simple"
    ],
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}